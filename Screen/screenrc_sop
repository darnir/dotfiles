source $HOME/.screenrc

screen -t "Latex"
stuff "cd $HOME/Documents/MS\ Applications/SOP/"
stuff "vim Statement_Of_Purpose.tex"

screen -t "Build"
stuff "cd $HOME/Documents/MS\ Applications/SOP/"
stuff "pdf Statement_Of_Purpose.pdf"
stuff "make"

select 0

